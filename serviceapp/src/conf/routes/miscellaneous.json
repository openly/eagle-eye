{
    "/" : {
        "module"   : "version"
    },
    "/version" : {
        "module" : "version"
    },
    "/static-content" : {
        "module"        : "static_content",
        "authorisation" : "user",
        "subroutes" : {
            "/:area" : {
                "authorisation" : "user",
                "method"        : "getContent" 
            },
            "/versions/:area" : { 
                "method"        : "getContentVersions", 
                "authorisation" : "admin" 
            },
            "/version/:area/:version" : { 
                "method"        : "getContentByVersion" , 
                "authorisation" : "admin"
            },
            "/activate/:area" : { 
                "method"        : "activateContentVersion",
                "req_method"    : "post", 
                "authorisation" : "admin"  
            },
            "/save/:area" : { 
                "method"        : "saveContent",
                "req_method"    : "post" , 
                "authorisation" : "admin"
            },
            "/delete/:area" : { 
                "method"        : "deleteContent",
                "req_method"    : "get" , 
                "authorisation" : "admin"
            }
        }
    }
}